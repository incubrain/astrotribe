{
  "files": [],
  "compilerOptions": {
    "composite": true,
    "paths": {
      "@ib/logger": ["libs/logger/src"],
      "@ib/logger/*": ["libs/logger/src/*"],
      "@ib/db-testing": ["libs/db-testing/src"],
      "@ib/db-testing/*": ["libs/db-testing/src/*"],
      "@website/*": ["apps/website/*"],
      "#layers/*": ["layers/*"],
      "@@astronera/auth/*": ["apps/auth/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "types": ["node", "vue"],
    "allowJs": true,
    "isolatedModules": true
  },
  "exclude": ["node_modules"]
}
