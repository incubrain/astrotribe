{
  "name": "astronera-monorepo",
  "private": true,
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.8.2"
  },
  "resolutions": {
    "primevue": "^4.0.7",
    "@primevue/nuxt-module": "^4.0.7"
  },
  "scripts": {
    "start": "nx serve website",
    "buildsnpm": "nx build website",
    "generate": "nx generate website",
    "start:all": "./scripts/start-all.sh",
    "start:auth": "./scripts/start-auth.sh",
    "start:website": "./scripts/start-website.sh",
    "start:app": "./scripts/start-app.sh",
    "start:admin": "./scripts/start-admin.sh",
    "start:monitor": "./scripts/start-monitoring.sh",
    "db:setup": "tsx scripts/db-setup.ts",
    "db:seed": "tsx scripts/db-seed.ts",
    "ci": "tsx ci/scripts/index.ts",
    "supa:gen:seed": " supabase db dump -f supabase/dump.sql --data-only",
    "supa:gen:types": "supabase gen types typescript --local > ./supabase/schema.gen.ts",
    "supa:func": "supabase functions serve --env-file ./supabase/.env.local --no-verify-jwt",
    "supa:lint": "supabase db lint",
    "supa:migrate": "supabase db diff --schema auth,public,storage,extensions,pg_catelog -f",
    "supa:migrate:repair": "supabase migration repair --status applied --debug",
    "build": "./scripts/build-all.sh",
    "test": "./scripts/run-tests.sh",
    "lint": "./scripts/run-lint.sh",
    "lint:fix": "./scripts/run-lint-fix.sh",
    "docker:up": "./scripts/docker-build.sh",
    "docker:website": "docker compose -f docker-compose.website.yml up --build",
    "docker:down": "docker compose down",
    "clean": "npm cache clean --force && npx nuxi cleanup && ./scripts/clean-packages.sh && nx clear-cache"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
