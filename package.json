  "scripts": {
    "debug:test": "echo \"Running from $npm_package_name\"",
    "install:all": "npm install --workspaces",
    "clean:modules": "rm -rf node_modules **/node_modules",
    "mig:fix": "npx tsx scripts/db/migrate/3.fix-migration.ts",
    "gen:api": "npx tsx scripts/api/create-api",
    "doc:strapi": "docker compose -p astronera-cms -f docker-compose.strapi.yml",
    "dev:api": "nx serve api",
    "dev:website": "nx serve website",
    "dev:auth": "nx serve auth-service --verbose",
    "dev:app": "nx run-many -t serve -p auth-service,main-app,api --parallel --verbose",
    "dev:monitoring": "nx serve monitoring-dashboard",
    "dev:admin": "--workspaces=false nx run-many -t serve -p admin-dashboard,auth-service,api --parallel --verbose",
    "dev:test": "nx serve build-works --verbose",
    "dev:all": "scripts/run-all.sh",
    "build:libs": "nx run-many -t build -p db-testing,logger --verbose",
    "build:layers": "scripts/build-layers.sh",
    "build:auth": "nx build auth-service",
    "build:admin": "nx build admin-dashboard",
    "build:all": "scripts/build-all.sh",
    "build:debug": "npx tsx scripts/build-with-logs.ts -- --project",
    "db:migrate": "npx tsx scripts/db/migrate/1.perform-migration.ts",
    "db:setup": "npx tsx scripts/db/init/2.main.ts",
    "db:clean": "npx tsx ./scripts/db/db-cleanup.ts",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,vue,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,vue,json,md,yml,yaml}\"",
    "types": "nx run crud:typecheck --verbose",
    "clean:nuxi": "scripts/nuxi-cleanup.sh",
    "staging": "railway link -p d2993a83-20e2-4658-b045-8202866cd11f",
    "ai:docs": "./scripts/docs-manager.sh",
    "ai:package": "npx tsx cli/index.ts",
    "ai:scan": "./scripts/scan-projects.sh",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test:watch --all",
    "test:coverage": "nx run-many --target=test:coverage --all",
    "ib:permissions": "npx tsx cli/update-rba-permissions.ts",
    "pack:feature": "npx tsx scripts/bundler/bundler-cli.ts search",
    "bundle": "npx tsx scripts/bundler/bundler-cli.ts",
    "backup:cms": "npx tsx scripts/cms/export-strapi.ts",
    "bundle:debug": "DEBUG=true npx tsx scripts/bundler/bundler-cli.ts",
    "ib": "npx tsx cli/index.ts"
  },
