    "@nx/vite": "^19.8.0",
    "@nx/web": "^19.8.0",
    "@nx/workspace": "^19.8.0",
    "@pinia/nuxt": "^0.5.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "^0.5.13",
    "@tailwindcss/typography": "^0.5.12",
