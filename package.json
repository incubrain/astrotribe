    "db:seed": "tsx scripts/db-seed.ts",
    "ci": "tsx ci/scripts/index.ts",
    "debug": "npx nuxi dev --inspect",
    "debug-build": "clinic doctor -- node --trace-warnings --max-old-space-size=8192 debug-build.js",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "dev": "nuxt dev",
    "cache:clean": "npm cache clean --force && npx nuxi cleanup",
    "type:check": "npx nuxi typecheck",
    "generate": "nuxt generate",
    "preview": "nuxt preview --hostname 127.0.0.1 --port 3000",
    "supa:gen:seed": " supabase db dump -f supabase/dump.sql --data-only",
    "supa:gen:types": "supabase gen types typescript --local > ./supabase/schema.gen.ts",
    "supa:func": "supabase functions serve --env-file ./supabase/.env.local --no-verify-jwt",
    "supa:lint": "supabase db lint",
    "supa:migrate": "supabase db diff --schema auth,public,storage,extensions,pg_catelog -f",
    "supa:migrate:repair": "supabase migration repair --status applied --debug",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
