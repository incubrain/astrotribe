    "supa:lint": "supabase db lint",
    "supa:migrate": "supabase db diff --schema auth,public,storage,extensions -f",
    "supa:migrate:repair": "supabase migration repair --status applied --debug",
    "lint": "eslint . --ext .ts,.js,.vue",
    "lint:fix": "eslint . --ext .ts,.js,.vue --fix"
