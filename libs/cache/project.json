{
  "name": "@ib/cache",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "root": "libs/cache",
  "sourceRoot": "libs/cache/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{workspaceRoot}/libs/cache/dist"],
      "options": {
        "main": "libs/cache/src/index.ts",
        "outputPath": "libs/cache/dist",
        "tsConfig": "libs/cache/tsconfig.json",
        "project": "libs/cache/package.json",
        "format": ["esm"],
        "generateExportsField": true,
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "compiler": "tsc",
        "assets": [
          {
            "glob": "*.md",
            "input": "libs/cache",
            "output": "."
          }
        ],
        "external": ["ioredis", "ms", "@astronera/db"]
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": ["@astronera/db"]
        }
      ]
    }
  }
}
