{
  "name": "strapi-supabase",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/strapi-supabase/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{workspaceRoot}/packages/strapi-supabase/dist"],
      "options": {
        "outputPath": "packages/strapi-supabase/dist",
