{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowJs": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "useDefineForClassFields": true,
    "types": [
      "node",
      "vue",
      "@nuxt/types",
      "@nuxt/content",
      "@nuxtjs/i18n",
      "@pinia/nuxt",
      "@types/node"
    ]
  }
}
