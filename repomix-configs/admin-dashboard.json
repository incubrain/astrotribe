{
    "output": {
      "filePath": "ai-context/admin-dashboard-context.txt",
      "style": "markdown",
      "headerText": "Admin Dashboard application context including shared libraries and layers",
      "showLineNumbers": true,
      "topFilesLength": 10
    },
    "include": [
      "apps/admin-dashboard/**/*.{ts,tsx,js,jsx,vue}",
      "apps/admin-dashboard/**/*.md",
      "libs/**/*.{ts,tsx,js}",
      "layers/**/*.{ts,tsx,js,vue}",
      "shared-runtime.config.ts"
    ],
    "ignore": {
      "useGitignore": true,
      "useDefaultPatterns": true,
      "customPatterns": [
        "**/node_modules/**",
        "**/dist/**",
        "**/coverage/**",
        "**/.nuxt/**",
        "**/tests/**",
        "**/*.test.*",
        "**/*.spec.*",
        "**/debug/**"
      ]
    },
    "security": {
      "enableSecurityCheck": true
    }
  }